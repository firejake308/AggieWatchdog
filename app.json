{
  "expo": {
    "name": "Aggie Watchdog",
    "slug": "aggie-watchdog",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#500000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.insertcoolnamehere.aggiewatchdog",
      "googleServicesFile": "./google-services.json",
      "versionCode": 4
    },
    "description": "Aggie Watchdog is a mobile app that notifies students at Texas A&M University when seats open up in a class that they want",
    "githubUrl": "https://github.com/firejake308/AggieWatchdog"
  }
}
